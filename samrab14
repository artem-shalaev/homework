#include <iostream>

using namespace std;
int main()
{
	int k;
	double Res, umn3;
	cout << "Enter k: ";
	cin >> k;
	double у[3];
	for (int i = 0; i < 3; i++)
	{
		cout << "Enter у" << i + 1 << " = ";
		cin >> у[i];
	}
	for (int i = 0; i < 3; i++)
	{
		pow3 = 1;
		if (у[i] < 0)
			Res = log(у[i] * у[i] + 2.7);
		else
			Res = log(у[i] + 2.7);
		for (int j = 0; j < k; j++)
		{
			pow3 = 2 * pow3 / 3 + Res / (3 * pow3 * pow3);
		}
		cout << "Pow (" << Res << ", 1.0/3.0) = " << pow3 << endl;
	}
}

