#include <iostream>
#include <vector>
using namespace std;

int main() {
    vector<int> a, b;
    int x, n;
    cin >> x;
    for (int i = 0; i < x; ++i) {
        cin >> x;
        a.push_back(x);
    }
    for (int i = 0; i < x; ++i) {
        if (a[i] < 0) {
            b.push_back(a[i]);
            cout << a[i] << " ";
        }
    }
    for (int i = 0; i < x; ++i) {
        if (a[i] >= 0) {
            b.push_back(a[i]);
            cout << a[i] << " ";
        }
    }
    cout << endl;
    return 0;
}
